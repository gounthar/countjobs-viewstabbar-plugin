<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.jenkins-ci.plugins</groupId>
    <artifactId>plugin</artifactId>
    <version>3.51</version>
    <relativePath />
  </parent>

  <properties>
    <jenkins.version>2.176.4</jenkins.version><!-- which version of Jenkins is this plugin built against? -->
    <java.level>8</java.level>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>

  <groupId>ws.albors</groupId>
  <artifactId>countjobs-viewstabbar</artifactId>
  <version>1.0.1-SNAPSHOT</version>
  <packaging>hpi</packaging>

  <url>https://github.com/jenkinsci/countjobs-viewstabbar-plugin</url>

  <scm>
    <connection>scm:git:ssh://github.com/jenkinsci/countjobs-viewstabbar-plugin.git</connection>
    <developerConnection>scm:git:ssh://git@github.com/jenkinsci/countjobs-viewstabbar-plugin.git</developerConnection>
    <url>https://github.com/jenkinsci/countjobs-viewstabbar-plugin</url>
  </scm>

  <developers>
    <developer>
      <id>ilbors</id>
      <name>Ignacio Albors</name>
    </developer>
  </developers>
  
  <!-- get every artifact through maven.glassfish.org, which proxies all the artifacts that we need -->
  <repositories>
    <repository>
      <id>repo.jenkins-ci.org</id>
      <url>https://repo.jenkins-ci.org/public/</url>
    </repository>
  </repositories>

  <pluginRepositories>
    <pluginRepository>
      <id>repo.jenkins-ci.org</id>
      <url>https://repo.jenkins-ci.org/public/</url>
    </pluginRepository>
  </pluginRepositories>
</project>
